<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy Value">
        <Item>
          <IdeaActionID>XDebugger.CopyValue</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insertar'</ShortCut>
          <Description>Copy Value</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'SMSCounterTest.testUnicodeEmoji'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Mayús+F9'</ShortCut>
          <Description>Debug 'SMSCounterTest.testUnicodeEmoji'</Description>
          <Count>14</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration or Usages">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B'</ShortCut>
          <Description>Declaration or Usages</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Mayús+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Mayús+F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Resume Program">
        <Item>
          <IdeaActionID>Resume</IdeaActionID>
          <ShortCut>'F9'</ShortCut>
          <Description>Resume Program</Description>
          <Count>17</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Into">
        <Item>
          <IdeaActionID>StepInto</IdeaActionID>
          <ShortCut>'F7'</ShortCut>
          <Description>Step Into</Description>
          <Count>13</Count>
        </Item>
      </Statistic>
      <Statistic Action="Step Over">
        <Item>
          <IdeaActionID>StepOver</IdeaActionID>
          <ShortCut>'F8'</ShortCut>
          <Description>Step Over</Description>
          <Count>83</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SMSCounterTest.testUnicode'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'SMSCounterTest.testUnicode'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'SMSCounterTest.testUnicodeEmoji'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'SMSCounterTest.testUnicodeEmoji'</Description>
          <Count>18</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>12</Count>
        </Item>
      </Statistic>
    </option>
    <option name="suppressed">
      <Statistic Action="Key Promoter X">
        <Item>
          <IdeaActionID>ActivateKeyPromoterXToolWindow</IdeaActionID>
          <Description>Key Promoter X</Description>
        </Item>
      </Statistic>
    </option>
  </component>
</application>